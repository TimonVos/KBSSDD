<Window x:Class="View.StartAssessment.StartingScreen"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ViewModel.StartAssessment;assembly=ViewModel"
        mc:Ignorable="d"
        Title="StartingScreen" Height="450" Width="800">

    <Window.Resources>
        <local:StartingScreen  x:Key="ViewModel"/>
    </Window.Resources>


    <Grid>
        <ListBox d:ItemsSource="{Binding Name}" Margin="330,80,330,137"/>
        <TextBlock HorizontalAlignment="Center" Margin="0,47,0,0" TextWrapping="Wrap" Text="Selecteer een project" VerticalAlignment="Top"/>
        <Button Content="Ga verder" HorizontalAlignment="Center" Margin="0,314,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
